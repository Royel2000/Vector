{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "vector",
  "version": "0.1.0",
  "identifier": "com.vector.calculadora",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "vector",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.icns", "icons/icon.ico", "icons/vite.svg"],
    "android": {
      "versionCode": 100
    }
  }
}
